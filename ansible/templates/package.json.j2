{
  "name": "{{ app_name }}",
  "version": "1.0.0",
  "description": "{{ app_name }} application with PostgreSQL and S3 integration",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "migrate": "npm run migrate:up",
    "migrate:up": "node migrations/migrate.js up",
    "migrate:down": "node migrations/migrate.js down",
    "seed": "node seeds/seed.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "aws-sdk": "^2.1400.0",
    "@aws-sdk/client-s3": "^3.350.0",
    "dotenv": "^16.1.4",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "s3",
    "aws",
    "{{ environment }}"
  ]
}